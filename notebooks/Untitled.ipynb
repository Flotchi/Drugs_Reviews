   "source": [
    "from tensorflow.keras import models\n",
    "from tensorflow.keras.layers import Embedding, LSTM, Dense,InputLayer\n",
    "\n",
    "def initialize_model():\n",
    "    model = models.Sequential()\n",
    "    model.add(InputLayer(shape=(200,)))\n",
    "    model.add(Embedding(input_dim=10000, output_dim=128))  \n",
    "    model.add(LSTM(64))  \n",
    "    model.add(Dense(10, activation='softmax'))  \n",
    "    \n",
    "    return model\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 18,
   "id": "1b626f12",
   "metadata": {
    "ExecuteTime": {
     "end_time": "2024-12-02T11:42:00.628907Z",
     "start_time": "2024-12-02T11:42:00.485354Z"
    }
   },
   "outputs": [],
   "source": [
    "model = initialize_model()"
   ]
  },
  {
